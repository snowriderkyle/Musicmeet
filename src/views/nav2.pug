html.no-js(lang='en')
	head
		// Standard Meta
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0')
		// Site Properties
		title musicmeet
		link(rel='stylesheet', type='text/css', href='dist/semantic.min.css')
		link(rel='stylesheet' href='style/mystyle.css')
		script(src='semantic/dist/semantic.min.js')
		script(src='assets/library/jquery.min.js')
		script(src='../dist/components/visibility.js')
		script(src='../dist/components/sidebar.js')
		script(src='../dist/components/transition.js')
		script(src='js/script.js')
	body
		// Page Contents
		.ui.large.top.fixed.hidden.menu
			.ui.container
				a.active.item Home
				a.item Work
				a.item Company
				a.item Careers
				.right.menu
					.item
						a.ui.button Log in
					.item
						a.ui.primary.button Sign Up
		// Sidebar Menu
		.ui.vertical.inverted.sidebar.menu
			a.active.item Home
			a.item Work
			a.item Company
			a.item Careers
			a.item Login
			a.item Signup
