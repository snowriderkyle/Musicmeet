html.no-js(lang='en')
	head
		// Standard Meta
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0')
		// Site Properties
		title musicmeet
		link(rel='stylesheet' href='style/bootstrap.min.css')
		link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css', integrity='sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7', crossorigin='anonymous')
		link(rel='stylesheet', type='text/css', href='dist/semantic.min.css')
		link(rel='stylesheet' href='style/mystyle.css')
		script(src='semantic/dist/semantic.min.js')
		script(src='assets/library/jquery.min.js')
		script(src='../dist/components/visibility.js')
		script(src='../dist/components/sidebar.js')
		script(src='../dist/components/transition.js')
		script(src='./js/script.js')
		script(src='js/custom.js')
		script(href='js/custom.js')
	body
		//Folowing Menu
		.ui.fixed.large.top.fixed.hidden.menu
			.ui.container
				a.active.item Home
					i.home.icon
				a.item Profile
					i.user.icon
				a.item Find
					i.search.icon
				.right.menu
					.item
						a(href='/logout').ui.inverted.blue.button Log out
					.item
						a(href='#').ui.inverted.blue.button Sign Up
		// Sidebar Menu
		.ui.fixed.vertical.inverted.sidebar.menu
			a.item Home
			a.item Work
			a.item Company
			a.item Careers
			a.item Login
			a.item Signup
		// Page Contents
		.pusher
			.ui.fixed.inverted.vertical.aligned.segment
				.ui.container
					.ui.large.secondary.inverted.pointing.menu
						a.toc.item
							i.sidebar.icon
						a(href='/').item
							i.home.icon
						a(href='/profile').item
							i.user.icon
						a(href='/search').item
							i.search.icon
						.right.item
						a(href='/logout').ui.inverted.blue.button Log out
						a(href='/newuser').ui.inverted.blue.button Sign Up
block content